<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--

-->

<hibernate-mapping package="org.hib.delete">

    <class name="Laptop" table="LAPTOP">
        <id name="lapId" column="LAPTOP_ID"/>
        
        <property name="manfName" column ="MANUFACT"/>
        
        <property name="displaySize" column ="DISPLAY_SIZE"/>
 
    </class>

</hibernate-mapping>

