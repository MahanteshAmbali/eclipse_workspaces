<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--

-->

<hibernate-mapping package="org.hib">

    <class name="Product" table="Product_Updation">
        <id name="productId" column="Product_Id"/>
        
        <property name="productName" column = "Product_name"/>
        
        <property name="productModelNo" column = "Prod_Model_No"/>

        <property name="productPrice" column = "Product_Price"/>
        
        <property name="productCondition" column = "Product_Condition"/>
        
        <property name="productAvailability" column = "Product_Availability"/>
        
    </class>

</hibernate-mapping>

